
# 로그 레벨 (DEBUG, INFO, WARNING, ERROR, CRITICAL 중 하나 선택)
LOG_LEVEL=INFO

# ZMQ 통신에 사용할 포트 번호
ZMQ_PORT=5555

# 상위 티커 개수 (숫자 입력, 예: 30)
TOP_TICKERS=30

# 작업 실행 간격 (분 단위, 예: 10, 30, 60)
MINUTE_INTERVAL=10

# 애플리케이션 버전
VERSION=1.0.0

# Mattermost Webhook URL
MATTERMOST_URL=https://example.mattermost.com/hooks/your-webhook-url